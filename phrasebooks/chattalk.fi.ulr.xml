<ulr id="chattalk" version="1.9" baselang="fi">
	<entries>
		<entry id="title">
			<attrib type="orth" author="Sir McDuck">ChatTalk</attrib>
		</entry>
		<entry id="description">
			<attrib type="orth" author="Sir McDuck">This phrasebook will teach a few phrases that are useful for chatting</attrib>
		</entry>
		<entry id="comingin">
			<attrib type="orth" author="Sir McDuck">Sisääntulo</attrib>
		</entry>
		<entry id="hi">
			<attrib type="orth" author="Sir McDuck">Hei</attrib>
		</entry>
		<entry id="helloeverybody">
			<attrib type="orth" author="Sir McDuck">Hei, kaikki</attrib>
		</entry>
		<entry id="goodmorning">
			<attrib type="orth" author="Sir McDuck">Hyvää huomenta (in the morning)/ Hyvää päivää (during the day)</attrib>
		</entry>
		<entry id="goodafternoon">
			<attrib type="orth" author="Sir McDuck">Hyvää iltapäivää</attrib>
		</entry>
		<entry id="goodevening">
			<attrib type="orth" author="Sir McDuck">Hyvää iltaa</attrib>
		</entry>
		<entry id="goodnight">
			<attrib type="orth" author="Sir McDuck">Hyvää yötä</attrib>
		</entry>
		<entry id="heythere">
			<attrib type="orth" author="Sir McDuck">Hei siellä</attrib>
		</entry>
		<entry id="startconv">
			<attrib type="orth" author="Sir McDuck">Keskustelun aloittaminen</attrib>
		</entry>
		<entry id="whatsnew">
			<attrib type="orth" author="Sir McDuck">Mitä uutta?</attrib>
		</entry>
		<entry id="whatsup">
			<attrib type="orth" author="Sir McDuck">Kuinka menee?</attrib>
		</entry>
		<entry id="whattopic">
			<attrib type="orth" author="Sir McDuck">Mistä sinä puhut? | Mikä on puheenaihe?</attrib>
		</entry>
		<entry id="howareyou">
			<attrib type="orth" author="Sir McDuck">Kuinka voit?</attrib>
		</entry>
		<entry id="howareyoupl">
			<attrib type="orth" author="Sir McDuck">Kuinka voitte?</attrib>
		</entry>
		<entry id="howarethings">
			<attrib type="orth" author="Sir McDuck">Kuinka asiat sujuvat? | Kuinka menee?</attrib>
		</entry>
		<entry id="personalq">
			<attrib type="orth" author="Sir McDuck">Henkilökohtaiset kysymykset</attrib>
		</entry>
		<entry id="wherefrom">
			<attrib type="orth" author="Sir McDuck">Mistä olet kotoisin, {käyttäjänimi}?</attrib>
		</entry>
		<entry id="newhere">
			<attrib type="orth" author="Sir McDuck">Oletko uusi täällä? | Onko tämä ensimmäinen kertasi täällä?</attrib>
		</entry>
		<entry id="languagesspeak">
			<attrib type="orth" author="Sir McDuck">Mitä kieliä sinä puhut?</attrib>
		</entry>
		<entry id="whereincountry">
			<attrib type="orth" author="Sir McDuck">Missä päin {maa} sinä asut?</attrib>
		</entry>
		<entry id="howoldareyou">
			<attrib type="orth" author="Sir McDuck">Kuinka vanha olet?</attrib>
		</entry>
		<entry id="languageslearn">
			<attrib type="orth" author="Sir McDuck">Mitä kieliä opettelet?</attrib>
		</entry>
		<entry id="talkingyou">
			<attrib type="orth" author="Sir McDuck">Itsestäsi puhuminen</attrib>
		</entry>
		<entry id="iamfrom">
			<attrib type="orth" author="Sir McDuck">Olen kotoisin {maa}</attrib>
		</entry>
		<entry id="ispeak">
			<attrib type="orth" author="Sir McDuck">Puhun {kielet}</attrib>
		</entry>
		<entry id="ispeakfluently">
			<attrib type="orth" author="Sir McDuck">Puhun {kielet.} äidinkielenäni, {kielet.} sujuvasti, {kielet} keskinkertaisesti, ja vähän {kielet}</attrib>
		</entry>
		<entry id="sorryidontspeak">
			<attrib type="orth" author="Sir McDuck">Anteeksi, en puhu {kieli}</attrib>
		</entry>
		<entry id="iamlearning">
			<attrib type="orth" author="Sir McDuck">Olen opettelemassa | Haluan oppia {kielet}</attrib>
		</entry>
		<entry id="finishconv">
			<attrib type="orth" author="Sir McDuck">Keskustelun lopettaminen</attrib>
		</entry>
		<entry id="nicetomet">
			<attrib type="orth" author="Sir McDuck">Oli mukava tavata sinut</attrib>
		</entry>
		<entry id="nicetotalk">
			<attrib type="orth" author="Sir McDuck">Oli mukavaa keskustella kanssasi</attrib>
		</entry>
		<entry id="ihavetogo">
