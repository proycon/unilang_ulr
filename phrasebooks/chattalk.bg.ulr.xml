	<entries>
		<entry id="title">
			<attrib type="orth" author="Paruha">ChatTalk</attrib>
		</entry>
		<entry id="description">
			<attrib type="orth" author="Paruha">This phrasebook will teach a few phrases that are useful for chatting</attrib>
		</entry>
		<entry id="comingin">
			<attrib type="orth" author="Paruha">Влизане:</attrib>
		</entry>
		<entry id="hi">
			<attrib type="orth" author="Paruha">Здравей</attrib>
		</entry>
