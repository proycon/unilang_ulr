<ulr id="justreadphrases" version="1.9" baselang="sk">
	<entries>
		<entry id="title">
			<attrib type="orth" author="1149" lastmodified="1226390311">Just read phrases</attrib>
		</entry>
		<entry id="1">
			<attrib type="orth" author="1149" lastmodified="1226390311">Milujem ťa</attrib>
		</entry>
		<entry id="2">
			<attrib type="orth" author="1149" lastmodified="1226390311">Búrka dosahuje pobrežie</attrib>
		</entry>
		<entry id="3">
			<attrib type="orth" author="1149" lastmodified="1226390311">Tamtí ľudia počujú hlasný zvuk.</attrib>
		</entry>
		<entry id="5">
			<attrib type="orth" author="1149" lastmodified="1226390311">Vidí malého vtáka. | Vidí vtáčika.</attrib>
		</entry>
		<entry id="6">
			<attrib type="orth" author="1149" lastmodified="1226390311">Nájdeme starú budovu.</attrib>
		</entry>
		<entry id="7">
			<attrib type="orth" author="1149" lastmodified="1226390311">Hľadám kľúče.</attrib>
		</entry>
		<entry id="8">
			<attrib type="orth" author="1149" lastmodified="1226390311">Dotýkam sa tvojej hlavy.</attrib>
		</entry>
		<entry id="9">
			<attrib type="orth" author="1149" lastmodified="1226390311">(On) chce nové biele auto.</attrib>
		</entry>
		<entry id="10">
			<attrib type="orth" author="1149" lastmodified="1226390311">(Ona) otvára rozbité okno.</attrib>
		</entry>
		<entry id="11">
			<attrib type="orth" author="1149" lastmodified="1226390622" variant="1">Silné dievča zatvorí ťažké čierne dvere.</attrib>
			<attrib type="orth" author="1149" lastmodified="1226390622">Silné dievča zavrie ťažké čierne dvere.</attrib>
		</entry>
		<entry id="12">
			<attrib type="orth" author="1149" lastmodified="1226390622" variant="1">Rieši ten strašný problém.</attrib>
			<attrib type="orth" author="1149" lastmodified="1226390622">Rieši ten hrozný problém.</attrib>
		</entry>
		<entry id="13">
			<attrib type="orth" author="1149" lastmodified="1226390622" variant="1">Nahnevaný zločinec stratil mnoho hodnotných peňazí.</attrib>
			<attrib type="orth" author="1149" lastmodified="1226390622">Nazlostený zločinec stratil veľa hodnotných peňazí.</attrib>
		</entry>
		<entry id="14">
			<attrib type="orth" author="1149" lastmodified="1226390622">Ochutnávajú to výborné jedlo.</attrib>
		</entry>
		<entry id="15">
			<attrib type="orth" author="1149" lastmodified="1226390622" variant="1">Policajný dôstojník je iného názoru.</attrib>
			<attrib type="orth" author="1149" lastmodified="1226390622">Policajný dôstojník má iný názor.</attrib>
		</entry>
		<entry id="16">
			<attrib type="orth" author="1149" lastmodified="1226390622">Kolegovia pijú studenú vodu a horúci čaj.</attrib>
		</entry>
		<entry id="17">
			<attrib type="orth" author="1149" lastmodified="1226390622">Bohatá firma predáva zelenú zeleninu.</attrib>
		</entry>
		<entry id="18">
			<attrib type="orth" author="1149" lastmodified="1226390622">Kupujú desať nových kníh</attrib>
		</entry>
		<entry id="19">
			<attrib type="orth" author="1149" lastmodified="1226390622">Bolí ho pravé ucho.</attrib>
		</entry>
		<entry id="20">
			<attrib type="orth" author="1149" lastmodified="1226390622" variant="1">Poznám toho vysokého človeka.</attrib>
			<attrib type="orth" author="1149" lastmodified="1226390622">Toho vysokého človeka poznám.</attrib>
		</entry>
		<entry id="21">
			<attrib type="orth" author="1149" lastmodified="1226390622">Štíhla dáma sleduje svojho tučného šéfa.</attrib>
		</entry>
		<entry id="22">
			<attrib type="orth" author="1149" lastmodified="1226390622">Rýchly počítač mení farbu.</attrib>
		</entry>
	</entries>
</ulr>