<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ulr PUBLIC "-//UniLang//Universal Language Resource 1.9 (Language-Specific Subfile)//EN"
  "http://dev.unilang.org/docs/ulr1.9_sub.dtd">
<ulr id="chattalk" version="1.9" baselang="ms">
	<entries>
		<entry id="title">
			<attrib type="orth">ChatTalk</attrib>
		</entry>
		<entry id="description">
			<attrib type="orth">This phrasebook will teach a few phrases that are useful for chatting</attrib>
		</entry>
		<entry id="comingin">
			<attrib type="orth">Masuk</attrib>
		</entry>
		<entry id="startconv">
			<attrib type="orth">Memulakan perbualan</attrib>
		</entry>
		<entry id="personalq">
			<attrib type="orth">Soalan peribadi</attrib>
		</entry>
		<entry id="wherefrom">
			<attrib type="orth">Awak berasal dari mana, {username}?</attrib>
		</entry>
		<entry id="newhere">
			<attrib type="orth">Awak baru di sini? | Kali pertama awak di sini?</attrib>
		</entry>
		<entry id="howoldareyou">
			<attrib type="orth">Berapa umur awak?</attrib>
		</entry>
		<entry id="languageslearn">
			<attrib type="orth">Bahasa apa yang awak pelajari?</attrib>
		</entry>
		<entry id="talkingyou">
			<attrib type="orth">Berbual mengenai diri sendiri</attrib>
		</entry>
		<entry id="iamfrom">
			<attrib type="orth">Saya dari [negeri].</attrib>
		</entry>
		<entry id="sorryidontspeak">
			<attrib type="orth">Maaf, saya tidak boleh bercakap {bahasa}</attrib>
		</entry>
		<entry id="iamlearning">
			<attrib type="orth">Saya sedang belajar | Saya ingin belajar {bahasa}</attrib>
		</entry>
		<entry id="finishconv">
			<attrib type="orth">Menamatkan perbualan</attrib>
		</entry>
		<entry id="nicetomet">
			<attrib type="orth">Gembira dapat berjumpa dengan awak.</attrib>
		</entry>
		<entry id="nicetotalk">
			<attrib type="orth">Gembira dapat berbual dengan awak.</attrib>
		</entry>
		<entry id="timedinner">
			<attrib type="orth">Masa untuk makan malam</attrib>
		</entry>
		<entry id="parting">
			<attrib type="orth">Beredar</attrib>
		</entry>
		<entry id="seeyoulater">
			<attrib type="orth">Jumpa lagi nanti</attrib>
		</entry>
		<entry id="seeyoutomorrow">
			<attrib type="orth">Jumpa lagi esok</attrib>
		</entry>
		<entry id="seenexttime">
			<attrib type="orth">Jumpa lagi lain kali</attrib>
		</entry>
		<entry id="goodnighteverybody">
			<attrib type="orth">Selamat malam semua</attrib>
		</entry>
		<entry id="byeall">
			<attrib type="orth">Selamat tinggal semua</attrib>
		</entry>
		<entry id="xtras">
			<attrib type="orth">Ekstra</attrib>
		</entry>
		<entry id="thanks">
			<attrib type="orth">Terima kasih</attrib>
		</entry>
		<entry id="thanksalot">
			<attrib type="orth">Ribuan terima kasih</attrib>
		</entry>
		<entry id="sorry">
			<attrib type="orth">Maaf</attrib>
		</entry>
		<entry id="lol">
			<attrib type="orth">Ketawa terbahak-bahak</attrib>
		</entry>
		<entry id="hehe">
			<attrib type="orth">Hehe</attrib>
		</entry>
	</entries>
</ulr>