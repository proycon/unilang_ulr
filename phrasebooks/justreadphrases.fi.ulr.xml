		<entry id="142">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Nainen hymyilee, ja mies suutelee häntä</attrib>
		</entry>
		<entry id="143">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Me emme edelleenkään ole tyytyväisiä hänen ratkaisuunsa</attrib>
		</entry>
		<entry id="144">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Tiedän, mitä mieltä olet</attrib>
		</entry>
		<entry id="145">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Nainen osaa leipoa leipää</attrib>
		</entry>
		<entry id="146">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Näen useita sanoja</attrib>
		</entry>
		<entry id="147">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Olen suurempi kuin sinä</attrib>
		</entry>
		<entry id="148">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Hän on yhtä pieni kuin sinä</attrib>
		</entry>
		<entry id="149">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Hän alkaa vihata kouluaan</attrib>
		</entry>
		<entry id="150">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Hän lopettaa tämän vaikean aineen opiskelun</attrib>
		</entry>
		<entry id="151">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">He lakkaavat ajattelmasta hallitusta</attrib>
		</entry>
		<entry id="152">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Jatkamme työtämme</attrib>
		</entry>
		<entry id="153">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">He valmistautuvat hylkäämään laivan</attrib>
		</entry>
		<entry id="154">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Minun täytyy unohtaa, mitä näin</attrib>
		</entry>
		<entry id="155">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Jos olet tyytyväinen tuohon omituiseen selitykseen, olet tyhmä mies</attrib>
		</entry>
		<entry id="156">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Matkustan maahanne, ellei sada</attrib>
		</entry>
		<entry id="157">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Sokea tyttö pystyi haistamaan erittäin hyvin</attrib>
		</entry>
		<entry id="158">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Eilen kaikki ongelmani olivat kaukana poissa</attrib>
		</entry>
		<entry id="159">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">En ole nähnyt häntä kolmeen vuoteen</attrib>
		</entry>
		<entry id="160">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Odotan hänen tulevan luokseni viideksi tunniksi</attrib>
		</entry>
		<entry id="161">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Päätin olla välittämättä heidän päätöksestänsä</attrib>
		</entry>
		<entry id="162">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Toistan päätelmäni uudelleen</attrib>
		</entry>
		<entry id="163">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Opetan oppilaita laskemaan kymmeneen</attrib>
		</entry>
		<entry id="164">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Voitko kääntää tämän tekstin englantiin?</attrib>
		</entry>
		<entry id="165">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Korjasit vahingon itse, kun edellisen kerran jouduit onnettomuuteen</attrib>
		</entry>
		<entry id="166">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Minun mielestäni on mahdotonta tunnistaa tarkoitus</attrib>
		</entry>
		<entry id="167">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Tuotko matkalaukkuni vai pitääkö minun kantaa se itse?</attrib>
		</entry>
		<entry id="168">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">En ole vielä kyennyt oppimaan tätä oppituntia</attrib>
		</entry>
		<entry id="169">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Tämä on varmaankin hyvin tavallinen virhe</attrib>
		</entry>
		<entry id="170">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Onneksi olen yleensä terve, kuitenkin, valitettavasti satutin jalkani</attrib>
		</entry>
		<entry id="171">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Sää on hieno, lisäksi aurinko on paistanut koko päivän</attrib>
		</entry>
		<entry id="172">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Tiedätkö tokko pystyy järjestämään viiniä juhliin?</attrib>
		</entry>
		<entry id="173">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Uskontoni on edelleen sama kuin ennenkin</attrib>
		</entry>
		<entry id="174">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Armeijasta ei ole hyötyä, jos ei ole sotaa, vaan rauha</attrib>
		</entry>
		<entry id="175">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Kansainvälinen sotilas ampuu viatonta matkustajaa</attrib>
		</entry>
		<entry id="176">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Ehkäpä se on epätodennäköistä että sellaista tulee koskaan tapahtumaan</attrib>
		</entry>
		<entry id="177">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Vaikuttaa ajalta lähteä pois</attrib>
		</entry>
		<entry id="178">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Ikääni pidetään salaisuutena</attrib>
		</entry>
		<entry id="179">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Tällä kurssilla meidän odotetaan käyttävän sanakirjaa ja laskinta</attrib>
		</entry>
		<entry id="180">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Minä pelkään suorittaa loppuun sen, minkä olen aloittanut</attrib>
		</entry>
		<entry id="181">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">En saa mahdollisuutta verrata minun piirrostani sinun piirrokseesi</attrib>
		</entry>
		<entry id="182">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Sähkölaite tuottaa paljon energiaa</attrib>
		</entry>
		<entry id="183">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Ehdotan olemaan menemättä pohjoiseen, vaan menemään etelään</attrib>
		</entry>
		<entry id="184">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Voin pahoin. Itse asiassa, voin huonommin kuin viime viikolla</attrib>
		</entry>
		<entry id="185">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Vauvat eivät tosiaan osaa vielä puhua</attrib>
		</entry>
		<entry id="186">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Seuraukset pitää myöntää välittömästi</attrib>
		</entry>
		<entry id="187">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Tiesin oikein hyvin toimiako vaiko ei</attrib>
		</entry>
		<entry id="188">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Hän iski suoraan puun läpi</attrib>
		</entry>
		<entry id="189">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Luokkatoverini leikkivät yhdessä, mutta minä istuin yksin nurkassa</attrib>
		</entry>
		<entry id="190">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Lapsi halusi olla kiltti heille, mutta he eivät kuitenkaan pitäneet hänestä</attrib>
		</entry>
		<entry id="191">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Opettajani kielsi minua puhumasta sellaisia</attrib>
		</entry>
		<entry id="192">
			<attrib type="orth" author="Liisi &amp; Sir McDuck">Minun äitini ajatteli aina, että vaadin lisähuomiota</attrib>
		</entry>
	</entries>
</ulr>