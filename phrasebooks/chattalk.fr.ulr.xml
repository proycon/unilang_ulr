		<entry id="languageslearn">
			<attrib type="orth">Quelles langues apprends-tu ?</attrib>
		</entry>
		<entry id="talkingyou">
			<attrib type="orth">Parler de soi :</attrib>
		</entry>
		<entry id="iamfrom">
			<attrib type="orth">Je viens de {pays}</attrib>
		</entry>
		<entry id="ispeak">
			<attrib type="orth">Je parles le {langue(s)}</attrib>
		</entry>
		<entry id="ispeakfluently">
			<attrib type="orth">Le {language} est ma langue maternelle, je parle le {langue(s)} couramment, le {langue(s)} assez bien, le {langue(s)} un peu.</attrib>
		</entry>
		<entry id="sorryidontspeak">
			<attrib type="orth">Désolé, je ne parle pas le {langue}</attrib>
		</entry>
		<entry id="iamlearning">
			<attrib type="orth">J'apprends | Je veux apprendre le {langue(s)}</attrib>
		</entry>
		<entry id="finishconv">
			<attrib type="orth">Terminer une conversation :</attrib>
		</entry>
		<entry id="nicetomet">
			<attrib type="orth">enchanté de t'avoir rencontré</attrib>
		</entry>
