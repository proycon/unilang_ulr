	<entries>
		<entry id="title">
			<attrib type="orth">ChatTalk</attrib>
		</entry>
		<entry id="description">
			<attrib type="orth">This phrasebook will teach a few phrases that are useful for chatting</attrib>
		</entry>
		<entry id="comingin">
			<attrib type="orth">Introduction :</attrib>
		</entry>
		<entry id="hi">
			<attrib type="orth">Salut</attrib>
		</entry>
		<entry id="helloeverybody">
			<attrib type="orth">Bonjour tout le monde</attrib>
		</entry>
		<entry id="goodmorning">
			<attrib type="orth">Bonjour</attrib>
		</entry>
		<entry id="goodafternoon">
			<attrib type="orth">Bonjour</attrib>
		</entry>
		<entry id="goodevening">
			<attrib type="orth">Bonsoir</attrib>
		</entry>
		<entry id="goodnight">
			<attrib type="orth">Bonne nuit</attrib>
		</entry>
		<entry id="heythere">
			<attrib type="orth">Salut</attrib>
		</entry>
