			<attrib type="orth">Engager une conversation :</attrib>
		</entry>
		<entry id="whatsnew">
			<attrib type="orth">Quoi de neuf ?</attrib>
		</entry>
		<entry id="whatsup">
			<attrib type="orth">Quoi de neuf ?</attrib>
		</entry>
