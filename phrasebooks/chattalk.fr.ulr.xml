			<attrib type="orth">Merci</attrib>
		</entry>
		<entry id="thanksalot">
			<attrib type="orth">Merci beaucoup !</attrib>
		</entry>
		<entry id="sorry">
			<attrib type="orth">Désolé(e)</attrib>
		</entry>
		<entry id="lol">
			<attrib type="orth">mdr (mort de rire)</attrib>
		</entry>
		<entry id="hehe">
			<attrib type="orth">Hé he | Ha ha !</attrib>
		</entry>
