			<attrib type="phon">'salve a 'tu:t:i</attrib>
		</entry>
		<entry id="goodmorning">
			<attrib type="orth">Buon giorno</attrib>
			<attrib type="phon">bwɔn 'dʒorno</attrib>
		</entry>
		<entry id="goodafternoon">
			<attrib type="orth">Buon pomeriggio</attrib>
			<attrib type="phon">'bwɔn pome'ri:dʒ:o</attrib>
		</entry>
		<entry id="goodevening">
			<attrib type="orth">buona sera</attrib>
			<attrib type="phon">'bwɔ:na 'se:ra</attrib>
		</entry>
