			<attrib type="phon">inits:'jare una conversa'ts:'jone</attrib>
		</entry>
		<entry id="whatsnew">
			<attrib type="orth">Novità?</attrib>
			<attrib type="phon">novi'ta</attrib>
		</entry>
		<entry id="whatsup">
			<attrib type="orth">Che succede?</attrib>
			<attrib type="phon">ke su'tʃ:ɛde</attrib>
		</entry>
