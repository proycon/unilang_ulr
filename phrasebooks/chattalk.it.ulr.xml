			<attrib type="phon">skuza | mi dis'pja:tʃe</attrib>
		</entry>
		<entry id="lol">
			<attrib type="orth">ridere a crepapelle</attrib>
			<attrib type="phon">ri'de:re a krɛpa'pɛl:e</attrib>
