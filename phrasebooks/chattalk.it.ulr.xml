		<entry id="personalq">
			<attrib type="orth">domande personali</attrib>
			<attrib type="phon">do'made personali</attrib>
		</entry>
		<entry id="wherefrom">
			<attrib type="orth">Di dove sei, {username}?</attrib>
			<attrib type="phon">di 'dove 'sɛi</attrib>
		</entry>
		<entry id="newhere">
			<attrib type="orth">Sei nuovo qui? (m) | Sei nuova qui? (f) | È la prima volta che sei qui?</attrib>
			<attrib type="phon">sɛi 'nwɔ:vo kwi | sɛi 'nwɔ:va kwi | ɛ la 'pri:ma 'vɔlta ke sɛi kwi</attrib>
		</entry>
