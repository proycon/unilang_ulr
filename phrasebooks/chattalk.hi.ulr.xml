			<attrib type="orth">namaste logon</attrib>
		</entry>
		<entry id="goodmorning">
			<attrib type="orth">subh prabhat</attrib>
		</entry>
		<entry id="goodafternoon">
			<attrib type="orth">subh sandhya/dopaher</attrib>
		</entry>
		<entry id="goodevening">
			<attrib type="orth">subh shaam</attrib>
		</entry>
		<entry id="goodnight">
			<attrib type="orth">subh ratri</attrib>
		</entry>
		<entry id="whattopic">
			<attrib type="orth">tum kis ke baare mein bol rahe ho</attrib>
		</entry>
