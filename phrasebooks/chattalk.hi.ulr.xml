			<attrib type="orth">aapse baat kar mujhe khushi hui</attrib>
		</entry>
		<entry id="ihavetogo">
			<attrib type="orth">mujhe jaana hai</attrib>
		</entry>
		<entry id="leavingtobed">
			<attrib type="orth">main pat mein jaa raha hoon</attrib>
		</entry>
