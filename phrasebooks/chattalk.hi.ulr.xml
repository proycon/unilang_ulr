		</entry>
		<entry id="thanks">
			<attrib type="orth">shukriya/dhanyavaad</attrib>
		</entry>
		<entry id="thanksalot">
			<attrib type="orth">bahut shukriya</attrib>
		</entry>
		<entry id="sorry">
			<attrib type="orth">mujhe maf kar do</attrib>
		</entry>
