			<attrib type="orth">maaf kijie,main ..nahin bolta hoon</attrib>
		</entry>
		<entry id="iamlearning">
			<attrib type="orth">main sikh raha hoon/main sikhna chahta hoon</attrib>
		</entry>
		<entry id="nicetomet">
			<attrib type="orth">aapse milke mujhe khushi hui</attrib>
		</entry>
