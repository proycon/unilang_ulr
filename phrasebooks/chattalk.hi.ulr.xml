			<attrib type="orth">kahan rehte ho tum ...mein</attrib>
		</entry>
		<entry id="howoldareyou">
			<attrib type="orth">tumne kitne umar ke ho/tum kitne saal ho</attrib>
		</entry>
		<entry id="languageslearn">
			<attrib type="orth">kya bhaashe tum sikhte ho</attrib>
		</entry>
		<entry id="talkingyou">
			<attrib type="orth">apne baare mein bolna</attrib>
		</entry>
