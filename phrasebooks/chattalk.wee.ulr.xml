<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ulr PUBLIC "-//UniLang//Universal Language Resource 1.9 (Language-Specific Subfile)//EN"
  "http://dev.unilang.org/docs/ulr1.9_sub.dtd">
<ulr id="chattalk" version="1.9" baselang="wee">
	<entries>
		<entry id="title">
			<attrib type="orth">ChatTalk</attrib>
		</entry>
		<entry id="description">
			<attrib type="orth">This phrasebook will teach a few phrases that are useful for chatting</attrib>
		</entry>
		<entry id="goodmorning">
			<attrib type="orth">(question) Dobre ranje! | (answer) Dobre spodobanje!</attrib>
		</entry>
		<entry id="goodafternoon">
			<attrib type="orth">Dobry dźeń!</attrib>
		</entry>
		<entry id="goodevening">
			<attrib type="orth">Dobry wječor!</attrib>
		</entry>
		<entry id="goodnight">
			<attrib type="orth">Dobru nóc!</attrib>
		</entry>
		<entry id="whatsnew">
			<attrib type="orth">Što je noweho?</attrib>
		</entry>
		<entry id="howareyou">
			<attrib type="orth">Kak so ći wjedźe?</attrib>
		</entry>
		<entry id="howareyoupl">
			<attrib type="orth">Kak so wam wjedźe?</attrib>
		</entry>
		<entry id="personalq">
			<attrib type="orth">Wosobinske podaća</attrib>
		</entry>
		<entry id="howoldareyou">
			<attrib type="orth">Kak stary (masc.) | stara (fem.) sy?</attrib>
		</entry>
		<entry id="iamfrom">
			<attrib type="orth">Sym z {country}</attrib>
		</entry>
		<entry id="thanks">
			<attrib type="orth">Dźakuju so</attrib>
		</entry>
	</entries>
</ulr>